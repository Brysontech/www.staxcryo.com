<style>
      #map {
        width: 100%;
        height: 600px;
        background-color: grey;
      }
    </style>
<div id="map"></div>

    <script>
      function initMap() {
        var uluru = {lat: 35.193706, lng: -80.8796197};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 14,
          center: uluru,
          draggable: false,
          scrollwheel: false
        });
        var marker = new google.maps.Marker({
          position: uluru,
          title: "Staxcryo",
          label: "3722 S Tryon St, Charlotte, NC 28217",
          map: map
        });
      }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDGz4b2DuJuHn3T_AyiW-am44l-ObGKyOg&callback=initMap">
    </script>